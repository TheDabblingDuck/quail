{
  "name": "Quail",
  "version": "0.1.11",
  "description": "Question Bank Browser",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "run-s build:*",
    "buildlinux": "electron-builder --linux",
    "buildmac": "electron-builder --mac",
    "releaselinux": "electron-builder --linux --publish always"
  },
  "build": {
    "appId": "TheDabblingDuck.quail",
    "productName": "Quail",
    "mac": {
      "category": "public.app-category.education"
    },
    "dmg": {
      "icon": "quail.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Education"
    }
  },
  "keywords": [],
  "author": "TheDabblingDuck",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-builder": "22.10.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "directory-tree": "^2.2.9",
    "electron-store": "^8.0.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "sanitize-html": "^2.3.3",
    "uuid": "^8.3.2"
  }
}
